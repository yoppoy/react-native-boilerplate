{
  "name": "yoppoy-boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:build:win": "cd android && gradlew assembleRelease",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:logcat:win": "%ANDROID_HOME%/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.2",
    "react": "16.9.0",
    "react-native": "0.61.2",
    "react-native-config": "^0.11.7",
    "react-redux": "^7.1.1",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.1",
    "reduxsauce": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/runtime": "^7.6.3",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "flow-bin": "^0.110.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-test-renderer": "16.9.0"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./Tests/jestSetupFile.js"
    ],
    "globals": {
      "window": {}
    }
  },
  "main": "index.js",
  "license": "MIT"
}
